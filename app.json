{
  "expo": {
    "name": "Lifeline Global",
    "slug": "emergencies",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/loogo.png",
    "userInterfaceStyle": "light",
    "splash": {
      "image": "./assets/loogo.png",
      "resizeMode": "contain",
      "backgroundColor": "#25274D"
    },
    "plugins": [
      [
        "expo-location",
        {
          "locationWhenInUsePermission": "Allow da app to use your location."
        }
      ]
    ],
    "ios": {
      "supportsTablet": true,
      "icon": "./assets/loogo.png",
      "config": {
        "usesNonExemptEncryption": false
      },
      "buildNumber": "21",
      "infoPlist": {
        "CFBundleIconName": "loogo",
        "NSLocationWhenInUseUsageDescription": "The app uses your location to grab your coordinates and tell what country you are in and what you have for local resources"
      },
      "bundleIdentifier": "com.pjf1822.emergencies"
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/adaptive-icon.png",
        "backgroundColor": "#ffffff"
      },
      "permissions": [
        "android.permission.ACCESS_COARSE_LOCATION",
        "android.permission.ACCESS_FINE_LOCATION"
      ]
    },
    "web": {
      "favicon": "./assets/favicon.png"
    },
    "extra": {
      "eas": {
        "projectId": "d0535f46-a81d-4519-a7cd-49d483a5745f"
      }
    },
    "runtimeVersion": {
      "policy": "appVersion"
    },
    "updates": {
      "url": "https://u.expo.dev/d0535f46-a81d-4519-a7cd-49d483a5745f"
    }
  }
}
